    "import pyautogui\n",
    "import clipboard\n",
    "import time\n",
    "\n",
    "time.sleep(3)\n",
    "pyautogui.press('f6')\n",
    "pyautogui.hotkey('ctrl', 'c') # for copying the selected url\n",
    "url = clipboard.paste()\n",
    "pyautogui.press('f6')\n",
    "pyautogui.press('f6')\n",
    "pyautogui.press('f6')\n",
    "print(url)"
